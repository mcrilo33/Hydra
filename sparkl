#Acces point : https://query.wikidata.org/bigdata/namespace/wdq/sparql?query={SPARQL}

SELECT ?album ?performer ?albumLabel ?publication_date WHERE {
  ?album wdt:P175 ?performer.
  ?performer rdfs:label "Aphex Twin"@en.
  ?album (wdt:P31/wdt:P279*) wd:Q482994.
  ?album wdt:P175 ?performer.
  ?album wdt:P577 ?publication_date.
  SERVICE wikibase:label { bd:serviceParam wikibase:language "en". }
}
